{
  "coc.preferences.formatOnSaveFiletypes": ["typescript", "javascript", "typescriptreact", "javascriptreact", "ruby"],
  "diagnostic-languageserver.filetypes": {
    "ruby": "rubocop"
  },
	"solargraph.diagnostics": true,
  "solargraph.autoformat": true,
