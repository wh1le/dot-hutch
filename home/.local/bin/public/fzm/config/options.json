{
  "general": {
    "terminal": {
      "name": "kitty",
      "bin": "/run/current-system/sw/bin/kitty",
      "socket_path": "/tmp/kitty-launcher",
      "launch_options": [
        "-o font_size=13",
        "-o allow_remote_control=yes",
        "--single-instance"
      ]
    },
    "desktop": {
      "name": "hyprland",
      "launcher_class": "launcher"
    },
    "picker": {
      "name": "fzf",
      "bin": "/run/current-system/sw/bin/fzf",
      "launch_options": [
        "--ansi",
        "--margin='1,1,1,1'",
        "--cycle",
        "--color='gutter:-1,border:238,scrollbar:238'",
        "--prompt='> '",
        "--bind 'esc:abort'",
        "--bind 'ctrl-d:preview-half-page-down'",
        "--bind 'ctrl-u:preview-half-page-up'",
        "--bind 'ctrl-f:preview-page-down'",
        "--bind 'ctrl-b:preview-page-up'"
      ]
    }
  },
  "styles": {
    "compact": {
      "desktop": {
        "hyprland": ["resizeactive 100 100", "centerwindow"],
        "sway": ["resize set 100 100", "move position center"]
      },
      "terminal": {
        "kitty": ["set-font-size 10"]
      }
    },
    "wide": {
      "desktop": {
        "hyprland": ["resizeactive 700 400", "centerwindow"],
        "sway": ["resize set 700 400", "move position center"]
      },
      "terminal": {
        "kitty": ["set-font-size 10"]
      }
    },
    "tall": {
      "desktop": {
        "hyprland": ["resizeactive 100 200", "centerwindow"],
        "sway": ["resize set 100 200", "move position center"]
      },
      "terminal": {
        "kitty": ["set-font-size 10"]
      }
    },
